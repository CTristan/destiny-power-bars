{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "no-bitwise": false,
    "interface-name": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "react/jsx-no-target-blank": [
      1,
      {
        "allowReferrer": true
      }
    ]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["src/serviceWorker.ts"]
  }
}
